{
  "appId": "com.sisoe.google-cloud-view",
  "productName": "Google Cloud View",
  "directories": {
    "output": "release"
  },
  "files": [
    {
      "from": "dist",
      "to": "dist",
      "filter": ["**/*"]
    },
    {
      "from": "dist-electron",
      "to": "dist-electron",
      "filter": ["**/*"]
    },
    "package.json"
  ],
  "win": {
    "target": "portable",
    "signAndEditExecutable": false
  }
}
